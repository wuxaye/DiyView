<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.CircleSeekBarActivity">
    
    <com.xaye.diyview.view.circleseekbar.CircleSeekBar
        android:id="@+id/circleSeekBar"
        android:layout_width="300dp"
        android:layout_height="300dp"
        app:wheel_max_process="100"
        app:wheel_can_touch="true"
        app:wheel_cur_process="60"
        app:wheel_has_wheel_shadow="true"
        app:wheel_scroll_only_one_circle="true"
        app:wheel_has_cache="true"
        android:layout_centerHorizontal="true"/>

</RelativeLayout>